@api.model
def default_get(self, fields_list):
    res = super().default_get(fields_list)

    if 'publication_date' in fields_list:
        res['publication_date'] = fields.Date.today()

    if 'price' in fields_list:
        res['price'] = 100.0

    if 'category_id' in fields_list:
        category = self.env['book.category'].search([], limit=1)
        if category:
            res['category_id'] = category.id

    return res
